{{ header }}{{ column_left }}
<div id="content">
  <div class="page-header">
    <div class="container-fluid">
      <div class="pull-right">
        <button type="submit" form="form-module" data-toggle="tooltip" title="{{ button_save }}" class="btn btn-primary"><i class="fa fa-save"></i></button>
        <a href="{{ cancel }}" data-toggle="tooltip" title="{{ button_cancel }}" class="btn btn-default"><i class="fa fa-reply"></i></a></div>
      <h1>{{ heading_title }}</h1>
      <ol class="breadcrumb">
        {% for breadcrumb in breadcrumbs %}
          <li class="breadcrumb-item"><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
        {% endfor %}
      </ol>
    </div>
  </div>
  <div class="container-fluid">
    {% if error_warning %}
      <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ error_warning }}
        <button type="button" class="close" data-dismiss="alert">&times;</button>
      </div>
    {% endif %}
    <div class="card">
      <div class="card-header"><i class="fa fa-pencil-alt"></i> {{ text_edit }}</div>
      <div class="card-body">
        <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-module">
          <div class="form-group row">
            <label class="col-sm-2 col-form-label" for="input-status">{{ entry_status }}</label>
            <div class="col-sm-10">
              <select name="module_brax_botao_download_status" id="input-status" class="form-control">
                {% if module_brax_botao_download_status %}
                  <option value="1" selected="selected">{{ text_enabled }}</option>
                  <option value="0">{{ text_disabled }}</option>
                {% else %}
                  <option value="1">{{ text_enabled }}</option>
                  <option value="0" selected="selected">{{ text_disabled }}</option>
                {% endif %}
              </select>
            </div>            
          </div>
          <div class="form-group row">
            <label class="col-sm-4 col-form-label" for="input-information_code_0">{{ entry_information_code_0 }}</label>
            <div class="col-sm-8">
              <input type="text" name="module_brax_botao_download_information_code_0" 
				value="{{ module_brax_botao_download_information_code_0 }}" 
				placeholder="{{ entry_information_code_0 }}" id="input-information_code_0" class="form-control"
			  />
              {% if error_information_code_0 %}
                <div class="invalid-tooltip">{{ error_information_code_0 }}</div>
              {% endif %}
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-4 col-form-label" for="input-information_code_1">{{ entry_information_code_1 }}</label>
            <div class="col-sm-8">
              <input type="text" name="module_brax_botao_download_information_code_1" 
				value="{{ module_brax_botao_download_information_code_1 }}" 
				placeholder="{{ entry_information_code_1 }}" id="input-information_code_1" class="form-control"
			  />
              {% if error_information_code_1 %}
                <div class="invalid-tooltip">{{ error_information_code_1 }}</div>
              {% endif %}
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-4 col-form-label" for="input-information_code_2">{{ entry_information_code_2 }}</label>
            <div class="col-sm-8">
              <input type="text" name="module_brax_botao_download_information_code_2" 
				value="{{ module_brax_botao_download_information_code_2 }}" 
				placeholder="{{ entry_information_code_2 }}" id="input-information_code_2" class="form-control"
			  />
              {% if error_information_code_2 %}
                <div class="invalid-tooltip">{{ error_information_code_2 }}</div>
              {% endif %}
            </div>
          </div>
          <div class="form-group row">
            <label class="col-sm-4 col-form-label" for="input-information_code_3">{{ entry_information_code_3 }}</label>
            <div class="col-sm-8">
              <input type="text" name="module_brax_botao_download_information_code_3" 
				value="{{ module_brax_botao_download_information_code_3 }}" 
				placeholder="{{ entry_information_code_3 }}" id="input-information_code_3" class="form-control"
			  />
              {% if error_information_code_3 %}
                <div class="invalid-tooltip">{{ error_information_code_3 }}</div>
              {% endif %}
            </div>
          </div>
		  
          <div class="form-group row">
            <label class="col-sm-4 col-form-label" for="input-information_code_4">{{ entry_information_code_4 }}</label>
            <div class="col-sm-8">
              <input type="text" name="module_brax_botao_download_information_code_4" 
				value="{{ module_brax_botao_download_information_code_4 }}" 
				placeholder="{{ entry_information_code_4 }}" id="input-information_code_4" class="form-control"
			  />
              {% if error_information_code_4 %}
                <div class="invalid-tooltip">{{ error_information_code_4 }}</div>
              {% endif %}
            </div>
          </div>
		  
		  
        </form>
      </div>
    </div>
  </div>
</div>
{{ footer }} 